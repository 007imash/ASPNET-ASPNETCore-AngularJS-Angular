    "@angular/http": "~2.3.0",
    "@angular/platform-browser": "~2.3.0",
    "@angular/platform-browser-dynamic": "~2.3.0",
    "@angular/router": "~3.3.0",
    "@angular/upgrade": "~2.3.0",
    "angular-in-memory-web-api": "~0.1.17",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.2",
    "@angular/platform-server": "~2.3.0",
    "bootstrap": "^3.3.7",
    "ie-shim": "^0.1.0",
    "jquery": "2.2.4"
  },
  "devDependencies": {
